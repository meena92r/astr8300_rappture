<?xml version="1.0"?>
<run>
    <tool>
        <title>Integrating and Graphing Calculator</title>
        <about>Press Simulate to view results.</about>
        <command>python @tool/yields.py @driver</command>
    </tool>
    <input>
        <string id="sp">
            <about>
                <label>Species</label>
                <hints>Example: o16</hints>
            </about>
            <size>30x5</size>
	    <default>o16</default>
        </string>
        <number id="Z">
            <about>
		<label>Z</label>
		<description>Value of metallicity</description>
	    </about>
            <default>0.01</default>
        </number>
    </input>
</run>
